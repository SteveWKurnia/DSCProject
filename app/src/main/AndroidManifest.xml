<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="android.wk.com.dscmovieproject">

    <application
        android:allowBackup="true"
        android:icon="@drawable/cinema_vector"
        android:label="@string/app_name"
        android:roundIcon="@drawable/cinema_vector"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:windowSoftInputMode = "adjustResize">
        <activity
            android:name=".loginPage"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".HomeFiles.contentDetails"
            android:parentActivityName=".HomeActivity"/>
        <activity android:name=".HomeActivity" />
        <activity android:name=".ProfileFiles.watchlistActivity" />
    </application>

</manifest>